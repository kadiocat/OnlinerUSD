{
   "manifest_version": 3,
   "name": "Onliner USD",
   "version": "0.01",
   "description": "Отображения цен onliner.by в USD",
   "icons": {
      "128": "128.png",
      "48": "48.png",
      "16": "16.png"
    },
   "content_scripts": [
       {
           "matches": [ "*://*.onliner.by/*" ],
           "js": [ "onliner-usd.js" ]
       }
   ],
   "permissions": [
    "storage"
  ]
}